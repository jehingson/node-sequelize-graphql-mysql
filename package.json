{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon src/server.js --exec babel-node",
    "build": "babel src/ --out-dir build",
    "server": "node build/server.js",
    "client": "cd client && run start",
    "server-install": "npm install",
    "client-install": "cd client && npm install",
    "install-all": "concurrently \" npm run server-install \" \" npm run client-install \"",
    "dev": "concurrently \" npm run server \" \" npm run client \"",
    "sequelize:create-migration": "cd src && npx sequelize migration:create",
    "sequelize:migrate": "cd src && npx sequelize-cli db:migrate",
    "heroku-postbuild": "cd client && npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "apollo-server": "^2.2.3",
    "apollo-server-core": "^3.1.2",
    "apollo-server-express": "^2.2.3",
    "concurrently": "^5.3.0",
    "bcrypt": "^3.0.8",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dataloader": "^1.4.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.0",
    "fs-capacitor": "^6.2.0",
    "graphql": "^14.0.2",
    "graphql-iso-date": "^3.6.1",
    "graphql-resolvers": "^0.3.2",
    "graphql-tools": "^2.3.0",
    "graphql-upload": "^13.0.0",
    "jsonwebtoken": "^8.4.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "npm-force-resolutions": "0.0.10",
    "sequelize": "^6.6.5",
    "sequelize-cli": "^6.4.1",
    "uuid": "^3.3.2",
    "validator": "^10.9.0"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0",
    "graphql-upload": "^12.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.8",
    "@babel/preset-env": "^7.9.11",
    "nodemon": "^2.0.15"
  }
}
